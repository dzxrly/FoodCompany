<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
    <!--
        name：即实体类的全名
        table：映射到数据库里面的那个表的名称
        catalog：数据库的名称
     -->
    <class name="Income" table="income" catalog="FoodCompany">
        <!-- class下必须要有一个id的子元素 -->
        <!-- id是用于描述主键的 -->
        <id name="incomeId" column="incomeId" type="java.lang.Integer">
            <!-- 主键生成策略 -->
            <generator class="native"></generator>
        </id>
        <!--
            使用property来描述属性与字段的对应关系
            如果length忽略不写，且你的表是自动创建这种方案，那么length的默认长度是255
        -->
        <property name="stuffNumber" column="stuffNumber" type="java.lang.Integer" length="11"></property>
        <property name="incomePrice" column="incomePrice" type="java.lang.Double" length="0"></property>
        <property name="incomeTime" column="incomeTime" type="java.lang.String" length="20"></property>
        <property name="incomeType" column="incomeType" type="java.lang.Integer" length="11"></property>
        <property name="incomeItem" column="incomeItem" type="java.lang.String" length="255"></property>
        <property name="payCardNumber" column="payCardNumber" type="java.lang.String" length="20"></property>
        <property name="orderId" column="orderId" type="java.lang.Integer" length="11"></property>
    </class>
</hibernate-mapping>