<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
    <!--
        name：即实体类的全名
        table：映射到数据库里面的那个表的名称
        catalog：数据库的名称
     -->
    <class name="ProductionDetailForm" table="productionDetailForm" catalog="FoodCompany">
        <!-- class下必须要有一个id的子元素 -->
        <!-- id是用于描述主键的 -->
        <id name="id" column="id" type="java.lang.Integer">
            <!-- 主键生成策略 -->
            <generator class="native"></generator>
        </id>
        <!--
            使用property来描述属性与字段的对应关系
            如果length忽略不写，且你的表是自动创建这种方案，那么length的默认长度是255
        -->

        <property name="productionId" column="productionId" type="java.lang.Integer" length="11"></property>
        <property name="orderId" column="orderId" type="java.lang.Integer" length="11"></property>
        <property name="goodsId" column="goodsId" type="java.lang.Integer" length="11"></property>
        <property name="goodsName" column="goodsName" type="java.lang.String" length="20"></property>
        <property name="productionQuantity" column="productionQuantity" type="java.lang.Double" length="11"></property>
        <property name="stocks" column="stocks" type="java.lang.Double" length="11"></property>
    </class>
</hibernate-mapping>